generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  userName String @db.VarChar(100)
  age      Int?
  email    String @db.VarChar(400)
  password String @db.VarChar(400)
}